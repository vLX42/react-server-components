{
    "version": 2,
    "build": {
      "env": {
        "BUILDING_FOR_NOW": "true"
      }
    },
    "builds": [
      { "src": "home/package.json", "use": "@vercel/next" },
      { "src": "client-component/package.json", "use": "@vercel/next" },
      { "src": "server-component/package.json", "use": "@vercel/next" }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/home/$1" }]
  }